@startuml
actor Client
participant "get_all_quotes_for_endorsements_by_quote" as View
participant "get_all_quote_history_by_quote_id"

Client -> View: POST /getcompletequotehistory
View -> View: Parse request body (quote_id)
View -> get_all_quote_history_by_quote_id: get_all_quote_history_by_quote_id(quote_id)
get_all_quote_history_by_quote_id --> View: res
alt res[0]['quote_status'] exists
    View -> View: res[0]['quote_status'] = res[0]['quote_status'].upper()
end
View -> Client: Response({result: res})
@enduml